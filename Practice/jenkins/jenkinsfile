pipeline {
   agent any
   stages {
       stage('clean up') {
          steps {
              deleteDir();
          }
        }
      stage('clone repo') {
          steps {
              sh "git config --global credential.username nitesh.46664@gmail.com"
              sh "git clone https://github.com/Mrnkk/Practice.git"
          }
        }
        stage('jar') {
          steps {
              sh "jar cvfe //wsl.localhost/docker-desktop/home/first.jar Temp1 ."
          }
        }
     }
  }
